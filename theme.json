{
	"$schema": "https://schemas.wp.org/wp/6.1/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"customGradient": true
		},
		"layout": {
			"contentSize": "720px",
			"wideSize": "1000px"
		},
		"spacing": {
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"customFontSize": true,
			"fontFamilies": [
				{
					"fontFace": [
						{
							"fontFamily": "Ubuntu",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/ubuntu_300.ttf"
							]
						},
						{
							"fontFamily": "Ubuntu",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/ubuntu_500.ttf"
							]
						},
						{
							"fontFamily": "Ubuntu",
							"fontStyle": "normal",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/ubuntu_700.ttf"
							]
						},
						{
							"fontFamily": "Ubuntu",
							"fontStyle": "italic",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/ubuntu_300italic.ttf"
							]
						},
						{
							"fontFamily": "Ubuntu",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/ubuntu_regular.ttf"
							]
						},
						{
							"fontFamily": "Ubuntu",
							"fontStyle": "italic",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/ubuntu_italic.ttf"
							]
						},
						{
							"fontFamily": "Ubuntu",
							"fontStyle": "italic",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/ubuntu_500italic.ttf"
							]
						},
						{
							"fontFamily": "Ubuntu",
							"fontStyle": "italic",
							"fontWeight": "700",
							"src": [
								"file:./assets/fonts/ubuntu_700italic.ttf"
							]
						}
					],
					"fontFamily": "Ubuntu",
					"slug": "ubuntu"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Rancho",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/rancho_regular.ttf"
							]
						}
					],
					"fontFamily": "Rancho",
					"slug": "rancho"
				},
				{
					"fontFace": [
						{
							"fontFamily": "Raleway",
							"fontStyle": "normal",
							"fontWeight": "300",
							"src": [
								"file:./assets/fonts/raleway_300.ttf"
							]
						},
						{
							"fontFamily": "Raleway",
							"fontStyle": "normal",
							"fontWeight": "500",
							"src": [
								"file:./assets/fonts/raleway_500.ttf"
							]
						},
						{
							"fontFamily": "Raleway",
							"fontStyle": "normal",
							"fontWeight": "900",
							"src": [
								"file:./assets/fonts/raleway_900.ttf"
							]
						},
						{
							"fontFamily": "Raleway",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/raleway_regular.ttf"
							]
						}
					],
					"fontFamily": "Raleway",
					"slug": "raleway"
				}
			]
		}
	},
	"styles": {
		"blocks": {
			"core/paragraph": {
				"spacing": {
					"margin": {
						"bottom": "0",
						"left": "0",
						"right": "0",
						"top": "0"
					},
					"padding": {
						"bottom": "var:preset|spacing|30",
						"top": "var:preset|spacing|30"
					}
				}
			}
		},
		"elements": {
			"button": {
				"color": {
					"background": "#0b0b0b"
				},
				"typography": {
					"lineHeight": "1.3"
				}
			},
			"h1": {
				"typography": {
					"fontFamily": "Rancho"
				}
			},
			"h2": {
				"typography": {
					"fontFamily": "Raleway"
				}
			},
			"h3": {
				"typography": {
					"fontFamily": "Raleway"
				}
			},
			"h4": {
				"typography": {
					"fontFamily": "Raleway"
				}
			},
			"h5": {
				"typography": {
					"fontFamily": "Raleway"
				}
			},
			"h6": {
				"typography": {
					"fontFamily": "Raleway"
				}
			},
			"heading": {
				"typography": {
					"fontFamily": "Rancho",
					"lineHeight": "1.3"
				}
			},
			"link": {
				"color": {
					"text": "#24b8e5"
				},
				"typography": {
					"textDecoration": "none"
				}
			}
		},
		"spacing": {
			"blockGap": "0px",
			"padding": {
				"bottom": "0px",
				"left": "0px",
				"right": "0px",
				"top": "0px"
			}
		},
		"typography": {
			"fontFamily": "var:preset|font-family|ubuntu",
			"lineHeight": "1.7"
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header"
		},
		{
			"area": "footer",
			"name": "footer"
		}
	],
	"version": 2
}